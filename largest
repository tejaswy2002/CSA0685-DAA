#include<stdio.h>
int  main ()
{
	int size,max,i;
	printf("enter the size of the array:");
	
	scanf("%d",&size);
	int arr[size];
	printf("enetr the element of array");
	for(i=0;i<size;i++)
	{
		scanf("%d",&arr[i]);
	}
	max=arr[0];
	for(i=1;i<size;i++)
	{
		if(max<arr[i])
		max=arr[i];
	}
	printf("largest number %d",max);
}
